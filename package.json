{
  "name": "matech-memento-api",
  "version": "1.4.7",
  "description": "Memento API",
  "author": "Matech Studios, LLC",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:windows": "rmdir dist /Q /S",
    "build:linux": "rm -rf dist",
    "migration:create": "yarn build && npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down",
    "generate:seed": "npx mikro-orm seeder:run",
    "generate:testdata": "yarn build && npx mikro-orm seeder:run --class=TestSeeder",
    "migrateAndGenerateTestData": "yarn migrate:up && yarn migrate:create && yarn migrate:up && yarn generate:testdata",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "npx rimraf dist temp",
    "start": " yarn clean && nest start",
    "predev": "yarn clean && yarn build && yarn migration:up",
    "dev": "nest start --watch",
    "debug": "yarn clean &&  nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:updatesnapshots": "jest --updateSnapshot",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:remove:volume": "docker volume ls | awk '/nestjs_db/ { print$2  }' | xargs -L1 docker volume rm",
    "docker:remove:image": "docker image rm --force matech-api",
    "docker:rebuild": " rm -rf dist test && npm install && docker-compose down && yarn docker:remove:volume && yarn docker:remove:image && docker-compose build --force-rm && docker-compose up",
    "docs:generate": "npx @compodoc/compodoc -p tsconfig.json -s "
  },
  "dependencies": {
    "@mikro-orm/cli": "5.7.14",
    "@mikro-orm/core": "5.7.14",
    "@mikro-orm/migrations": "5.7.14",
    "@mikro-orm/nestjs": "5.1.2",
    "@mikro-orm/postgresql": "5.7.14",
    "@mikro-orm/reflection": "5.7.14",
    "@mikro-orm/seeder": "5.7.14",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.3",
    "@types/crypto-js": "^4.1.1",
    "argon2": "^0.30.2",
    "aws-sdk": "^2.1312.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "firebase-admin": "^11.10.1",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "mustache": "^4.2.0",
    "nest-aws-sdk": "^3.0.1",
    "nestjs-i18n": "^10.2.6",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/mime-types": "^2.1.1",
    "@types/mustache": "^4.2.2",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "28.1.3",
    "pactum": "^3.2.4",
    "prettier": "^3.0.0",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "mikro-orm": {
    "useTsNode": true
  }
}
